{
    "name": "directus-extension-drawer-notice",
    "version": "1.0.5",
    "description": "A notice field that is only visible in the drawer",
    "license": "GPL-3.0",
    "author": {
        "email": "dev@formfcw.com",
        "name": "formfcw"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/formfcw/directus-extension-drawer-notice"
    },
    "icon": "info",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-interface",
        "notice",
        "drawer"
    ],
    "type": "module",
    "directus:extension": {
        "host": "^10.0.0 || ^11.0.0",
        "type": "interface",
        "path": "dist/index.js",
        "source": "src/index.ts"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build-publish": "directus-extension build && npm publish",
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "^12.0.2",
        "typescript": "^5.2.2",
        "vue": "^3.3.5",
        "vue-i18n": "^10.0.1"
    }
}
